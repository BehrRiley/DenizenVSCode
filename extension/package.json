{
    "name": "denizenscript",
    "displayName": "DenizenScript",
    "description": "The Denizen scripting language.",
    "author": "mcmonkey and the DenizenScript team",
    "version": "0.1.0",
    "publisher": "denizenscript",
    "repository": "https://github.com/DenizenScript/DenizenVSCode",
    "engines": {
        "vscode": "^1.33.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:denizenscript"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "languages": [
            {
                "id": "denizenscript",
                "aliases": [
                    "Denizen Script",
                    "denizenscript"
                ],
                "extensions": [
                    ".dsc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "configurationDefaults": {
            "[denizenscript]": {
                "editor.insertSpaces": true
            }
        },
        "scripts": {
            "vscode:prepublish": "npm run compile",
            "compile": "tsc -p ./"
        },
        "devDependencies": {
            "@types/node": "^8.10.25",
            "typescript": "^2.6.1"
        }
    }
}
